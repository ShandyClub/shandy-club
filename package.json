{
  "name": "shandy-club",
  "version": "2.0.0",
  "description": "Shandy Club - curated pubs for curious people",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShandyClub/shandy-club.git"
  },
  "keywords": [
    "shandy",
    "club",
    "beer",
    "pubs"
  ],
  "author": "Shandy Club <cheers@shandy.club>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ShandyClub/shandy-club/issues"
  },
  "homepage": "http://shandy.club/",
  "lint-staged": {
    "*.js": [
      "eslint --fix \"app/**/*.js\" \"pages/**/*.js\"",
      "git add"
    ]
  },
  "pre-commit": {
    "run": [
      "lint:staged",
      "test"
    ],
    "silent": true
  },
  "scripts": {
    "test": "exit",
    "lint:staged": "lint-staged",
    "start": "per-env",
    "start:development": "next",
    "start:production": "next start",
    "build": "next build",
    "deploy": "yarn build && xervo deploy -p shandyclub .next/dist/"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-styled-components": "^1.1.4",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "faucet": "0.0.1",
    "lint-staged": "^3.4.0",
    "pre-commit": "^1.1.3",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "color": "^0.11.4",
    "deep-equal": "^1.0.1",
    "immutable": "^3.7.6",
    "immutable-props": "^1.1.0",
    "leaflet": "^1.0.1",
    "leaflet.markercluster": "^1.0.0",
    "mixpanel-browser": "^2.9.15",
    "next": "^2.1.1",
    "next-redux-wrapper": "^1.0.0",
    "per-env": "^1.0.2",
    "react": "^15.0.1",
    "react-addons-css-transition-group": "^15.3.2",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.2",
    "redux": "^3.4.0",
    "redux-analytics": "^0.3.1",
    "redux-immutablejs": "0.0.8",
    "redux-logger": "^2.6.1",
    "redux-pack": "^0.1.5",
    "regenerator-runtime": "^0.9.5",
    "reselect": "^2.3.0",
    "styled-components": "^2.0.0-14",
    "uuid": "^3.0.1"
  }
}
